<!DOCTYPE html>

<html>
<head>
	<link href="css/base.min.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="css/styles.css"/>

	


	<meta charset="utf-8">
	<meta content="IE=edge" http-equiv="X-UA-Compatible">
	<meta content="initial-scale=1.0, width=device-width" name="viewport">
	
	<title>Letz Go</title>
</head>
<body>
<audio autoplay loop>
  <source src="sounds/track.mp3" type="audio/mpeg">
  Your browser does not support the audio tag.
</audio>

<div class="container">
<!-- <div class="content" style="float:left"> -->
	<ul id="scene" class="scene">
      <li class="layer" data-depth="0.90"><img src="images/layer1.png"></li>
      <li class="layer" data-depth="0.80"><img src="images/layer2.png"></li>
      <li class="layer" data-depth="0.60"><img src="images/layer3.png"></li>
      <li class="layer" data-depth="0.60"><img src="images/layer4.png"></li>
      <li class="layer" data-depth="0.40"><img src="images/layer5.png"></li>
      <li class="layer" data-depth="0.20"><img src="images/layer6.png"></li>
    </ul>
    <br>
    <div>
    	<input class="button" type="button" id="relative" value="Explore">
    </div style="padding-right: 50px;">
	    <div style="visibility: hidden;">
		    <input type="checkbox" id="clip" >
		    <label for="clip">clipRelativeInput</label>
		</div>
  </div>
    <div class="fbtn-container" align="center" ><!-- floating button -->
		<div class="fbtn-inner" align="center">
			
			<a align="center" class="fbtn fbtn-red fbtn-lg waves-circle waves-effect waves-light" data-toggle="dropdown" title="Upload" style="width:100px; height: 100px; font-size: 40px ;"><span align="center" class="access-hide" >Action</span><span class="fbtn-ori icon icon-add" style="padding-top:11px;"></span><span class="fbtn-sub icon icon-close " style="padding-top:11px; padding-left: -7px;"></span></a>
			
			<div class="fbtn-dropdown" style="padding-right: 30px; ">
				<a class="fbtn fbtn-blue waves-circle waves-effect waves-light" href="javascript:void(0)" style="width:70px; height: 70px; font-size: 40px ; padding-top:10px; ">
				<span class="fbtn-text">Log In</span><span class="icon icon-account-box" style=" padding-top:4px; padding-left:2px; "></span></a>
				
				<a class="fbtn fbtn-yellow waves-circle waves-effect waves-light" href="javascript:void(0)" style="width:70px; height: 70px; font-size: 40px ;"><span class="fbtn-text" >Register</span><span class="icon icon-person-add" style=" padding-top:2px; padding-right:3px;"></span></a>
			</div>
		</div>
	</div><!-- floating button end -->

<!-- </div>
 -->
</div>

	
	






	



	<script src="js/jquery.js"></script>
	<script src="js/jquery.parallax.js"></script>
	<script src="./js/parallax.js"></script>
  <script>

  // Elements
  var scene = document.getElementById('scene');
  var clipCheckbox = document.getElementById('clip');
  var relativeCheckbox = document.getElementById('relative');

  // Pretty simple huh?
  var parallax = new Parallax(scene, {
    relativeInput: relativeCheckbox.checked,
    clipRelativeInput: clipCheckbox.checked
  });

  relativeCheckbox.addEventListener('change', function(event) {
    parallax.relativeInput = relativeCheckbox.checked;
  });

  clipCheckbox.addEventListener('change', function(event) {
    parallax.clipRelativeInput = clipCheckbox.checked;
  });

  </script>
  <script src="js/base.min.js" type="text/javascript"></script>
</body>
</html>